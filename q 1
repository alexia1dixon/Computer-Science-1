#include <iostream>
#include <stdio.h>
using namespace std;

int main()
{
    cout<<"Enter a number (0-9)"<<endl; //ask user for number
    int num1=0; //create int variable
    cin>>num1; //get number from user

    cout<<"Enter another number (0-9)"<<endl; //ask user for number
    int num2=0; //create int variable
    cin>>num2; //get number from user
    
    cout<<"Enter another number (0-9)"<<endl; //ask user for number
    int num3=0; //create int variable
    cin>>num3; //get number from user
    
    int num4=num1*num2*num3; // tells the computer what to do with the numbers 
    cout << "The produce is"<<num4<< endl;// gives you your answer 
    return 0;
}
